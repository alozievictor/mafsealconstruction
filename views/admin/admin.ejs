                      fill-rule="evenodd"
                      d="M18 5v8a2 2 0 01-2 2h-5l-5 4v-4H4a2 2 0 01-2-2V5a2 2 0 012-2h12a2 2 0 012 2zM7 8H5v2h2V8zm2 0h2v2H9V8zm6 0h-2v2h2V8z"
                      clip-rule="evenodd"
                    ></path>
                  </svg>
                </div>
                <div>
                  <p class="mb-2 text-sm font-medium text-gray-600">
                    Total Request
                  </p>
                  <p class="text-lg font-semibold text-gray-700"><%- TotalQuestion %></p>
                </div>
              </div>
            </div>

            <div
              class="w-full grid md:grid-cols-5 gap-2 md:gap-4 my-5 overflow-hidden"
            >
              <div class="col-auto md:col-span-3">
                <div class="max-w-2xl mx-auto">
                  <div class="flex flex-col">
                    <div class="overflow-x-auto shadow-md rounded-lg">
                      <div class="inline-block min-w-full align-middle">
                        <div class="overflow-hidden">
                          <table
                            class="min-w-full divide-y divide-gray-200 table-fixed "
                          >
                            <thead class="bg-gray-100">
                              <tr>
                                <th
                                  scope="col"
                                  class="py-3 px-6 text-sm font-medium tracking-wider text-left text-gray-800 uppercase"
                                >
                                  Project Name
                                </th>
                                <th
                                  scope="col"
                                  class="py-3 px-6 text-sm font-medium tracking-wider text-left text-gray-800 uppercase"
                                >
                                  Category
                                </th>
                                <th
                                  scope="col"
                                  class="py-3 px-6 text-sm font-medium tracking-wider text-left text-gray-800 uppercase"
                                >
                                  Price
                                </th>
                                <th scope="col" class="p-4">
                                  <span class="sr-only">Edit</span>
                                </th>
                              </tr>
                            </thead>
                            <% Project.forEach( i=> { %>
                            <tbody
                              class="bg-white divide-y divide-gray-200"
                            >
                              <tr
                                class="hover:bg-gray-100"
                              >
                                
                                <td
                                  class="py-4 px-6 text-sm font-medium text-gray-600 whitespace-nowrap"
                                >
                                  Apple Imac 27"
                                </td>
                                <td
                                  class="py-4 px-6 text-sm font-medium text-gray-600 whitespace-nowrap"
                                >
                                  Desktop PC
